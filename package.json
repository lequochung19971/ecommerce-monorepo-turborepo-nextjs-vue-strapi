{
  "name": "nextjs-strapi",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:cms": "yarn build --filter=cms",
    "build:ecommerce-app": "yarn build --filter=ecommerce-app",
    "dev": "turbo run dev",
    "dev:web": "yarn workspace ecommerce-app start",
    "dev:cms": "yarn workspace cms start",
    "start:cms": "yarn start --filter=cms",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "dependencies": {},
  "packageManager": "npm@8.3.1"
}
